{
  "name": "git-api-read-cache",
  "version": "1.0.0",
  "description": "A node express server to cache and serve custom views for Github Apis",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js $PORT",
    "dev": "nodemon --exec npm restart",
    "restart": "rimraf dist && npm run build && npm start",
    "build": "babel src --out-dir dist --source-maps",
    "test": "./node_modules/jest/bin/jest.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ankushbansal89/git-api-read-cache.git"
  },
  "keywords": [
    "Express",
    "node",
    "github"
  ],
  "author": "Ankush Bansal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ankushbansal89/git-api-read-cache/issues"
  },
  "homepage": "https://github.com/ankushbansal89/git-api-read-cache#readme",
  "dependencies": {
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.6.0",
    "parse-link-header": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^24.9.0",
    "nodemon": "^1.19.3",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0"
  }
